<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ctgu.bs_hotel.mapper.HotelMapper">
    <update id="setMinPrice">
        update `hotel`
        set `hotel`.hotel_price = #{param2}
        where `hotel`.hotel_id = #{param1}
    </update>
</mapper>
